    "lint-staged": "^15.2.7",
    "ng-lint-staged": "^12.0.4",
    "prettier": "^2.8.8",
    "source-map-explorer": "^2.5.3",
    "ts-jest": "^29.1.4",
    "ts-node": "10.9.2",
    "typescript": "^4.9.5",
    "webpack": "^5.93.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "lint-staged": {
    "*!(.ts)": "prettier --cache --ignore-unknown -w",
    "*.ts": [
      "prettier --cache --ignore-unknown -w",
      "ng-lint-staged lint --fix --"
    ]
  }
}
